{
  "name": "panda-gazebo",
  "version": "2.0.18",
  "description": "Gazebo panda simulation created for the Panda environment that is found in the 'openai_ros' ROS package.",
  "keywords": [
    "reinforcement-learning",
    "simulation",
    "openai-gym",
    "ros",
    "gazebo",
    "panda",
    "gym-environments",
    "emika-franka"
  ],
  "homepage": "https://github.com/rickstaa/panda-gazebo#readme",
  "bugs": {
    "url": "https://github.com/rickstaa/panda-gazebo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickstaa/panda-gazebo.git"
  },
  "license": "UNLICENSED",
  "author": "Rick Staa",
  "scripts": {
    "prepare": "husky install",
    "changelog": "HUSKY=0 standard-version --skip.bump --skip.commit --skip.tag",
    "release": "HUSKY=0 standard-version",
    "sort": "sort-package-json"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.md": "./node_modules/.bin/remark -o .",
    "!(**/(tests)/**)*.py": [
      "flake8",
      "black"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.4",
    "lint-staged": "11.2.4",
    "remark-cli": "10.0.0",
    "remark-lint": "9.1.0",
    "remark-preset-lint-recommended": "6.1.1",
    "sort-package-json": "1.52.0",
    "standard-version": "^9.3.2"
  }
}
