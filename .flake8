[flake8]
max-line-length = 89
extend-ignore = E203
exclude =
    .git,
    __pycache__,
    sandbox,
    docs,
    tests,
    franka_ros,
    panda_moveit_config
per-file-ignores =
    openai_ros/openai_ros/src/openai_ros/task_envs/task_envs_list.py: E501
    __init__.py: F401, E501
max-complexity = 10
